{
    "name": "stackflows-com/php-client-stackflows",
    "description": "Stackflows Gateway API",
    "keywords": [
        "stackflows-client",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/stackflows-com/php-client-stackflows",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Stackflows\\GatewayApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Stackflows\\GatewayApi\\Test\\" : "test/" }
    },
    "minimum-stability": "dev"
}
